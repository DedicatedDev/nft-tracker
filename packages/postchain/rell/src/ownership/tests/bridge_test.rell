@test module;
import ownership.*;

function test_add_contract()  {
    log(rell.test.keypairs.alice);
    rell.test.tx().op(ownership.bridge.add_contract("eth_main", "34d344", "ERC1155")).sign(rell.test.keypairs.bob).run();
