
entity contract {
    key chain, address;
    chain: text;
    address: byte_array;
    type: text;
}

entity owner {
    key id: byte_array;
}

entity nft {
    key chain, contract, id;
    chain: name;
    contract: byte_array;
    id: integer;
}

entity ownership {
    index owner, nft;
}

entity current_owner {
    key nft;
    index ownership;
}



